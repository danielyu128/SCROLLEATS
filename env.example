# Google Places API
REACT_APP_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Yelp API
REACT_APP_YELP_API_KEY=your_yelp_api_key_here

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id_here
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_app_id_here

# AI Recommendations API (Gemini/Vertex AI)
REACT_APP_AI_API_ENDPOINT=https://your-ai-endpoint.com/api

# App Configuration
REACT_APP_APP_NAME=ScrollEats
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_AI_RECOMMENDATIONS=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_HAPTIC_FEEDBACK=true 